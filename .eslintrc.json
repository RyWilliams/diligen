{
  "extends": "airbnb",
  "rules": {
      "no-unused-vars": ["error", { "args": "none" }],
      "no-console": "off",
      "max-len": ["error", { "code": 120, "ignoreStrings": true }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/no-did-mount-set-state": "off",
      "react/require-default-props": "off"
  },
  "env": {
      "browser": true
  }
}
